<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor
        configVersion="3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.phpdoc.org"
        xsi:noNamespaceSchemaLocation="https://docs.phpdoc.org/latest/phpdoc.xsd"
>
    <paths>
        <output>docs</output>
    </paths>
    <version number="3.1.2">
        <api>
            <source dsn=".">
                <path>src</path>
            </source>
            <ignore hidden="true" symlinks="true">
                <path>build/**/*</path>
                <path>examples/**/*</path>
                <path>tests/**/*</path>
                <path>vendor/**/*</path>
            </ignore>
            <extensions>
                <extension>php</extension>
            </extensions>
            <default-package-name>tangram</default-package-name>
        </api>
    </version>
    <template name="default"/>
</phpdocumentor>
